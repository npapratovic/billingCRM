<script type="text/javascript">
    $(document).ready(function() {
        new WOW().init();

        <?php if(count($productsentries) > 0): ?> 
            <?php foreach($productsentries as $entry): ?>
                $("#btn-delete-product-id-<?php echo $entry->id; ?>").click(function() { 
                    $('#delete-product-id-<?php echo $entry->id; ?>').modal('show');
                });
            <?php endforeach; ?>
        <?php endif; ?> 

        <?php if(count($invoicesentries) > 0): ?> 
            <?php foreach($invoicesentries as $entry): ?>
